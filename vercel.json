{
  "buildCommand": "cd medo-app && npm install && npm run build",
  "outputDirectory": "medo-app/build",
  "devCommand": "cd medo-app && npm start",
  "installCommand": "cd medo-app && npm install && cd ../Backend && pip install -r requirements.txt",
  "functions": {
    "Backend/app.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/Backend/app.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
