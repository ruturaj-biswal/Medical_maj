{
  "version": 2,
  "builds": [
    {
      "src": "medo-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "Backend/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/Backend/app.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/medo-app/build/static/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|json|txt))",
      "dest": "/medo-app/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/medo-app/build/index.html"
    }
  ]
}
