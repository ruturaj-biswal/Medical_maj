{
  "buildCommand": "cd medo-app && npm install && npm run build",
  "outputDirectory": "medo-app/build",
  "devCommand": "cd medo-app && npm start",
  "framework": null,
  "functions": {
    "Backend/app.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/Backend/app.py" },
    { "source": "/(.*)", "destination": "/medo-app/build/$1" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "$1", "status": 200 }
  ]
}
